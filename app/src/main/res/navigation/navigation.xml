<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.turizmoprograma.home.HomeFragment"
        android:label="@string/home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_HomeFragment_to_RoutePlannerFragment"
            app:destination="@id/nav_route" />
        <action
            android:id="@+id/action_HomeFragment_to_TourismLocationsFragment"
            app:destination="@id/nav_locations" />
        <action
            android:id="@+id/action_HomeFragment_to_InteractiveMapFragment"
            app:destination="@id/nav_map" />
        <action
            android:id="@+id/action_HomeFragment_to_TourismHelperFragment"
            app:destination="@id/nav_helper" />
        <action
            android:id="@+id/action_HomeFragment_to_HelpFragment"
            app:destination="@id/nav_help" />
        <action
            android:id="@+id/action_HomeFragment_to_AboutFragment"
            app:destination="@id/nav_about" />
    </fragment>

    <fragment
        android:id="@+id/nav_route"
        android:name="com.example.turizmoprograma.route_planner.RoutePlannerFragment"
        android:label="@string/route"
        tools:layout="@layout/fragment_route_planner">
        <action
            android:id="@+id/action_RoutePlannerFragment_to_HomeFragment"
            app:destination="@id/nav_home" />
