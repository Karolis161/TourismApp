            app:destination="@id/nav_about" />
        <action
            android:id="@+id/action_TourismLocationsFragment_to_TourismLocationsDetailFragment"
            app:destination="@id/nav_locationDetail" />
    </fragment>

    <fragment
        android:id="@+id/nav_map"
        android:name="com.example.turizmoprograma.interactive_map.InteractiveMapFragment"
