            android:id="@+id/action_RoutePlannerFragment_to_HelpFragment"
            app:destination="@id/nav_help" />
        <action
            android:id="@+id/action_RoutePlannerFragment_to_AboutFragment"
            app:destination="@id/nav_about" />
        <action
            android:id="@+id/action_RoutePlannerFragment_to_RoutePlannerDetailFragment"
            app:destination="@id/nav_routeDetail" />
    </fragment>

    <fragment
        android:id="@+id/nav_locations"
        android:name="com.example.turizmoprograma.tourism_locations.TourismLocationsFragment"
        android:label="@string/locations"
        tools:layout="@layout/fragment_tourism_locations">
        <action
